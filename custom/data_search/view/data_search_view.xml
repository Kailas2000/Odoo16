<?xml version="1.0" encoding="utf-8" ?>
<odoo>
       <!--Form View    -->
    <record id="data_search_form_view" model="ir.ui.view">
        <field name="name">data.search.form</field>
        <field name="model">data.search</field>
        <field name="arch" type="xml">
            <form string="Data Search">
                <header>
                    <button name="action_search" string="Search" type="object"
                            class="btn-primary" data-hotkey="g"/>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="data"/>
                            <field name="model_id" options="{'no_create': True}"/>
                            <field name="fields_id" options="{'no_create': True}"/>
                        </group>
                    </group>
                    <field name="type_ids">
                        <tree string="Type">
                            <field name="data"/>
                            <field name="model"/>
                        </tree>
                    </field>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
