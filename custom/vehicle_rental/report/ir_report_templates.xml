<odoo>
    <template id="report_vehicle_rent">
       <t t-call="web.html_container">
           <t t-call="web.external_layout">
               <div class="page">
                   <div class="oe_structure"/>
                        <h2>VEHICLE RENTAL REPORT</h2>
                            <span t-if="data.get('from_date') != False">
                                <p><b>From Date: </b><span t-esc="data.get('from_date')"/></p>
                            </span>
                            <span t-if="data.get('to_date') != False">
                                <p><b>To Date: </b><span t-esc="data.get('to_date')"/></p>
                            </span>
                            <span t-if="data.get('name') != False">
                                <p><b>Vehicle Name: </b><span t-esc="data.get('name')[1]"/></p>
                            </span>
                    <table class="table table-sm" name="vehicle_rental_table">
                        <thead>
                            <tr>
                                <th name="th_sm_sl"><strong>Sl.No</strong></th>
                                <th name="th_sm_customer"><strong>Customer Name</strong></th>
                                <th name="th_sm_model"><strong>Model</strong></th>
                                <th name="th_sm_days"><strong>No: of Days</strong></th>
                                <th name="th_sm_state"><strong>State</strong></th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-set="i" t-value="1" />
                                <t t-foreach="report" t-as="dynamic_rental_report">
                                    <tr>
                                        <td style="text-align:center;">
                                            <span><t t-esc="i"/></span>
                                        </td>
                                        <td style="text-align:center;">
                                            <span><t t-esc="dynamic_rental_report['name']"/></span>
                                        </td>
                                        <td style="text-align:center;">
                                            <span><t t-esc="dynamic_rental_report['model']"/></span>
                                        </td>
                                        <td style="text-align:center;">
                                            <span><t t-esc="dynamic_rental_report['period']"/></span>
                                        </td>
                                        <td style="text-align:center;">
                                            <span><t t-esc="dynamic_rental_report['state']"/></span>
                                        </td>
                                    </tr>
                                    <t t-set="i" t-value="i+1"/>
                                </t>
                        </tbody>
                    </table>
               </div>
           </t>
       </t>
    </template>
</odoo>